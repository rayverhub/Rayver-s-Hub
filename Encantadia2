local Tab = Window:CreateTab("Abilities", 4483362458) -- Title, Image
local Button = Tab:CreateButton({
   Name = "Earthquake",
   Callback = function()
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("EarthquakeActivate"):FireServer()
   end,
})

local Button = Tab:CreateButton({
   Name = "Mass Burn",
   Callback = function()
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("MassBurnEvent"):FireServer()
   end,
})

local Button = Tab:CreateButton({
   Name = "Heal",
   Callback = function()
        local args = {
"Self"
}
game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("LupaHealRequest"):FireServer(unpack(args))
   end,
})

local Button = Tab:CreateButton({
   Name = "Shockwave",
   Callback = function()
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("MitenaShockwave"):FireServer()
   end,
})

local Button = Tab:CreateButton({
   Name = "Shapeshift (Pirena Only!)",
   Callback = function()
        game.Players.LocalPlayer.PlayerGui.Pirena.Shapeshift.Visible = true
   end,
})
